@article{schonhardt_optimization_2008,
	title = {Optimization of an electromagnetic comb drive actuator},
	issn = {09244247},
	url = {http://dx.doi.org/10.1016/j.sna.2008.08.007},
	doi = {10.1016/j.sna.2008.08.007},
	abstract = {We present the first electromagnetic comb drive actuator which allows for over 100Â Î¼m of linear actuator motion at large air gap widths of 25Â Î¼m at 300 coil windings, a structural height of 400Â Î¼m and for an applied current of 8Â mA. Previously reported designs require very narrow air gaps of 3Â Î¼m at structural heights of up to 1Â mm [K. Fischer, H. Guckel, Long throw linear magnetic actuators stackable to one millimeter of structural height, Microsyst. Technol., 4 (1998) 180â€“183] which makes them sensitive to operate, and challenging to fabricate. The new design reduces the reluctance by a parallel connection and cascading of several pole shoes as is well known for electrostatic actuators. This reduces the current consumption and linearizes the currentâ€“displacement characteristic. The cascaded design further enhances the displacement range at the cost of a slightly increased current consumption at the onset of the stroke. To analyze the influence of the number of air gaps on the plunger motion the total cross-sectional area of the magnetic core is kept constant while the number of air gaps is varied. Magnetic and mechanical material properties are measured for an accurate simulation of the system behavior. Experimental results confirm the theoretical predictions.},
	journal = {Sensors and Actuators A: Physical},
	author = {Schonhardt, S. and Korvink, J. and Mohr, J. and Hollenbach, U. and Wallrabe, U.},
	month = aug,
	year = {2008},
	keywords = {comb-drive, mems, notes, valve}
}

@article{schmidt_precision_2009,
