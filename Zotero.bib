@article{carlsson_bone_1994,
	title = {Bone response to hydroxyapatite-coated and commercially pure titanium implants in the human arthritic knee},
	volume = {12},
	url = {file://E:%5CEndNote%5CJournal%20Articles%5Cby%20Author%5Ccarlsson_bone%20response%20to%20hydroxyapatite%20coated%20and%20commercially%20pure%20titanium%20implants%20in%20the%20human%20arthritic%20knee.pdf},
	abstract = {Rough and smooth commercially pure (c.p.) titanium implants and hydroxyapatite-coated (HA-C) implants were inserted in arthritic human knees and left in situ in order to compare the response of bone to these three implants. Radiographic examination alone could not determine if bone apposition had occurred. After 3 and 6 months, histomorphometric analyses of undecalcified sections, 10 microns thick, revealed a statistical significance in the amount of bone apposition to rough-surfaced and HA-C implants as compared with smooth uncoated implants. Most of the rough-surfaced c.p. titanium implants and the HA-C implants had achieved bone apposition on the order of 50\%. No statistically significant differences in apposition were seen between the rough-surfaced and HA-C implants. The smooth c.p. titanium implants were mostly encapsulated in fibrous tissue.},
	number = {2},
	journal = {Journal of Orthopaedic Research},
	author = {Carlsson, L. and Regner, L. and Johansson, C. and Gottlander, M. and Herberts, P.},
	year = {1994},
	keywords = {humans, knee, prosthesis, titanium},
	pages = {274--85+}
}

@article{carstens_plastic_1971,
	title = {Plastic stress relaxation around solid inclusions in pyrope},
	volume = {32},
	url = {http://dx.doi.org/10.1007/BF00373346},
	doi = {10.1007/BF00373346},
	abstract = {Misfit between primary (syngenetic) inclusions and pyrope host is usually due to differences in the coefficients of thermal expansion and compressibilities. It is shown that the misfit stresses are relieved by the generation and rearrangement of dislocations. Zones of plastic flow have been recognized around inclusions in pyropes from Bohemian garnet peridotites and in kimberlite pyropes from South Africa and Yakutia. The extent of plastic yield is determined by the history of the pyrope in the ductile regime. Implications for using the piezothermometric method of Rosenfeld and Chase (1961) which is based upon the elastic strain, are discussed.},
	number = {4},
	journal = {Contributions to Mineralogy and Petrology},
	author = {Carstens, Harald},
