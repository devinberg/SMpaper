@article{keller_current_2013,
	title = {Current {Evidence} in {Gastrointestinal} {Surgery}: {Natural} {Orifice} {Translumenal} {Endoscopic} {Surgery} ({NOTES})},
	url = {http://dx.doi.org/10.1007/s11605-013-2277-3},
