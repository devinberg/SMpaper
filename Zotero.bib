	keywords = {exploration, gastrointestinal, notes},
	pages = {169--179}
}

@book{tadano_evaluation_2007,
	title = {Evaluation of a master slave system with force sensing using pneumatic servo system for laparoscopic surgery},
	url = {http://dx.doi.org/10.1109/sice.2007.4421479},
	abstract = {In the teleoperated minimally invasive surgery systems, measurement and display of sense of force to the operator is a problem. We have been developing a master slave system with force sensing using pneumatic actuators at the slave side. The system has 7-DOFs and is able to estimate external force without force sensors using pneumatic cylinders. In this paper, firstly, the slave manipulator is modified using a pneumatic rotary actuators to make the system more compact for controlling the tip part of the forceps. Then, the force control type of impedance control is adopted without force sensor for the pneumatic slave manipulator. For the master manipulator the motion control type of impedance control is implemented. Finally, the effectiveness of the system is evaluated with some experiments.},
	author = {Tadano, K. and Kawashima, K. and Kagawa, T.},
	year = {2007},
	keywords = {force, laparoscopy, notes, sensing}
}

@book{webster_toward_2006,
	title = {Toward {Active} {Cannulas}: {Miniature} {Snake}-{Like} {Surgical} {Robots}},
	url = {http://dx.doi.org/10.1109/iros.2006.282073},
	abstract = {We have developed a new class of continuously flexible snake-like robots, called active cannulas, that consist of several telescoping pre-curved superelastic tubes. The devices derive bending actuation not from tendon wires or other external mechanisms, but from elastic energy stored in the backbone itself. This allows active cannulas to have a small diameter and a high degree of dexterity, which should enable them to navigate through complex anatomy to sites inaccessible by current surgical robotic devices. Active cannulas may also enhance patient safety because their inherent compliance mitigates potential trauma from inadvertent tool-tissue collision. A consequence of our design is that dexterity improves with miniaturization. A kinematic description of active cannula shape requires a model of the elastic interaction of telescoping pre-curved flexible tubes, and we derive a two-link beam mechanics-based model. Experiments using curved nitinol tubes and wires validate the model},
	author = {Webster, R. J. and Okamura, A. M. and Cowan, N. J.},
	year = {2006},
	keywords = {notes, robot, surgical}
}

@article{yu_robotic_2006,
	title = {Robotic assistance provides excellent outcomes during the learning curve for laparoscopic {Roux}-en-{Y} gastric bypass: results from 100 robotic-assisted gastric bypasses.},
	volume = {192},
	issn = {0002-9610},
	url = {http://dx.doi.org/10.1016/j.amjsurg.2006.08.038},
	doi = {10.1016/j.amjsurg.2006.08.038},
	abstract = {BACKGROUND: Laparoscopic Roux-en-Y gastric bypass (LRYGB) has a reported learning curve of 100 cases. Robotic-assisted surgery decreases the learning curve in complex laparoscopic surgeries. We hypothesize that robotic-assisted, hand-sewn gastrojejunostomy during a LRYGB will improve results during the initial 100 cases when compared with the literature. METHODS: Our first 100 robotic-assisted gastrojejunostomies performed during LRYGB were reviewed from a prospective database. Patient demographics, operative times, length of stay, reoperations, anastomotic leak, pulmonary embolus, and death were all evaluated. RESULTS: The mean age and body mass index were 42 and 50, respectively. Operative times ranged from 148 minutes to 437 minutes (mean = 254). There were no leaks or deaths. Four patients had complications, including reoperation (1), incisional hernia (1), pulmonary embolus (1), and recurrent umbilical hernia (1). CONCLUSIONS: Robotic-assisted LRYGB is feasible and safe, as evidenced by the excellent outcomes from this series during the initial learning curve for LRYGB.},
	number = {6},
	journal = {American journal of surgery},
	author = {Yu, S. C. and Clapp, B. L. and Lee, M. J. and Albrecht, W. C. and Scarborough, T. K. and Wilson, E. B.},
	month = dec,
	year = {2006},
	keywords = {bypass, gastric, laparoscopy, notes, robotic},
	pages = {746--749}
}

