}

@article{phillips_femoral_1990,
	title = {Femoral stem fixation in hip replacement. {A} biomechanical comparison of cementless and cemented prostheses},
	volume = {72},
	issn = {0301-620X},
	abstract = {A leading problem with cementless hip replacement is thigh pain, probably due to some degree of loosening. This is most pronounced during activities such as stair climbing or getting up from a chair. Our study compares the immediate fixation of three stems in widespread clinical use, using a cadaver test simulation in which the femur was horizontal, and cyclic forces acted vertically downward on the prosthetic head with gradual increments of load. We implanted and tested 18 pairs of femurs, making analyses between stem types by right to left comparisons. We found no statistically significant differences in loosening between cementless AML and PCA prostheses, but cemented stems withstood greater loads than uncemented stems by a factor of five or more.},
