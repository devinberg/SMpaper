	keywords = {dynamic, kinematics, manipulator, model, research, robotic},
	pages = {1497--1502}
}

@inproceedings{ueda_design_2007,
	title = {Design of {PZT} cellular actuators with power-law strain amplification},
	isbn = {978-1-4244-0912-9},
	url = {http://dx.doi.org/10.1109/iros.2007.4399299},
	doi = {10.1109/iros.2007.4399299},
	abstract = {The demand for high-force and compact actuators with large strain is increasing in robotics. PZT is known as one of the promising materials for this purpose with respect to bandwidth, stress, and reliability. However, the most critical drawback of PZT is its extremely small strain, i.e. only 0.1 \%. This paper presents a nested rhombus structure for strain amplification of PZT stack actuators. This structure provides a "power-law" strain amplification, resulting in over 20 \% strain, which is particularly useful for gaining a large strain in a compact body, appropriate for many robotic applications. The notational convention and fundamental force-displacement analysis will be presented. In addition, the feasibility of the design concept will be addressed through the design of a proof- of-concept prototype.},
