@article{magnussen_impact_2000,
	title = {The impact of the use of inquiry-based learning as a teaching methodology on the development of critical thinking},
	volume = {39},
	issn = {0148-4834},
	abstract = {Problem-based learning (PBL) uses patients' problems to develop students' problem-solving and clinical skills. Inquiry-based learning (IBL) was developed as a similar methodology that was more holistic and flexible. This study sought to determine if inquiry-based learning (IBL) enhances critical-thinking ability as measured by the Watson Glaser Critical Thinking Appraisal (WGCTA). The WGCTA was administered to 228 nursing students in the first semester and 257 students in the final semester of their program. When the scores were stratified into groups, the students in the low group showed a significant increase in mean score, no change in the medium group, and a significant drop for the high group.},
