}

@article{shimada_grasping_2003,
	title = {Grasping power by means of functional electrical stimulation in a case of {C}6 complete tetraplegia},
	volume = {201},
	url = {file://C:%5Cgeneral%20reference%5Corthotic%5CTohoku%20178956.pdf},
	abstract = {Grasping power (GP) by means of functional electrical stimulation (FES) was measured in a case of C6 complete tetraplegia. This was compared with GP by means of the dynamic tenodesis effect, the flexor hinge splint and the GP of normal female. Palmar grasp strength by means of FES was approximately 16\% of the control group and 2.4 times greater than the flexor hinge splint. Lateral grasp strength by FES was approximately 13\% of the control group. Our results suggest that FES is more effective than the flexor hinge splint in increasing the GP of tetraplegic patients, and that a stronger and stable GP, which is not affected by wrist position, makes FES practical for improving activities of daily living (ADL).},
