	author = {Caldwell, D. G. and Medrano-Cerda, G. A. and Goodwin, M.},
	month = aug,
	year = {2002},
	keywords = {actuator, actuators, control, controls, modeling, notes},
	pages = {40--48}
}

@book{caldwell_characteristics_1994,
