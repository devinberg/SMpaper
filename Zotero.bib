	abstract = {The Da Vinci Surgical System is a requisite investment and a strong foundation for any minimally invasive surgery (MIS) initiative. For hospitals, the Da Vinci Surgical System enables the clinical and economic benefits of MIS to be applied to a broader base of surgical patients. A technology assessment of the Da Vinci Surgical System will demonstrate broad, hospital-wide benefit in the areas of market share growth, utilization mix, and productivity. The Da Vinci Surgical System can contribute to the hospitalâÂ€Â™s pursuit of profitability in three distinct ways: by growing the hospitalâÂ€Â™s market share and contributing to revenue growth with new surgical referrals; by helping to reduce system-wide costs through improved patient outcomes associated with minimally invasive surgery; and by helping to minimize length of stay.},
	booktitle = {Robotic {Urology}},
	publisher = {Springer Berlin Heidelberg},
	author = {Peplinski, R. and Rhodes, R.},
	editor = {John,, Hubert and Wiklund,, Peter},
	year = {2008},
	keywords = {da\_vinci, history, notes, robotic, surgery},
	pages = {253--261--261}
}

