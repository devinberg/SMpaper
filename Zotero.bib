@article{huiskes_validation_1997,
	title = {Validation of adaptive bone-remodeling simulation models},
	volume = {40},
	url = {file://E:%5CEndNote%5CJournal%20Articles%5Cby%20Author%5Chuiskes_validaiton%20of%20adaptive%20bone%20remodeling%20simulation%20models.pdf},
