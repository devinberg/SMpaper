@article{les_estimation_1994,
	title = {Estimation of material properties in the equine metacarpus with use of quantitative computed tomography},
	volume = {12},
	url = {file://C:%5Cgeneral%20reference%5CJournal%20Articles%5CEstimation%20of%20material%20properties%20in%20the%20equine%20metacarpus%20with%20use%20of%20quantitative%20CT.pdf},
	abstract = {The purpose of this study was to investigate the relationships between data obtained from quantitative computed tomography and mechanical properties in the equine metacarpus, as measured in vitro in bone specimens. Three hundred and fifty-five bone specimens from the metacarpi of 10 horses were machined into right cylinders aligned with the long axis of the bone. A computed tomographic scan of the specimens, along with a Cann-Genant K2HPO4 calibration standard, was obtained. The specimens then were compressed to failure, and the elastic modulus, yield stress, yield strain, strain energy density at yield, ultimate stress, ultimate strain, and strain energy density at ultimate failure were calculated. The specimens were dried and ashed. Quantitative computed tomography-derived K2HPO4 equivalent density proved to be an excellent estimator (r2 {\textgreater} 0.9) of elastic modulus, yield stress, ultimate stress, wet density, dry density, and ash density; a moderately good estimator (0.4 {\textless} r2 {\textless} 0.9) of strain energy density at yield and at ultimate failure; and a poor estimator (r2 {\textless} 0.2) of yield strain and ultimate strain. It was concluded that the relationships between quantitative computed tomography data and mechanical properties of the equine metacarpus were strong enough to justify the use of these data in automated finite element modeling.},
	number = {6},
	journal = {Journal of Orthopaedic Research},
	author = {Les, C. M. and Keyak, J. H. and Stover, S. M. and Taylor, K. T. and Kaneps, A. J.},
	year = {1994},
	keywords = {analysis, animals, biomechanics, bone, computed, density, female, govt, male, non-us, phs, regression, support, tomography, x-ray},
	pages = {822--33+}
}

